				egoldCardinal.setAttribute("data-to", (ajaxRet.egold).toString().split(".")[0]);
				egoldDecimal.setAttribute("data-to", (ajaxRet.egold).toString().split(".")[1]);
				esilverCardinal.setAttribute("data-to", (ajaxRet.esilver).toString().split(".")[0]);
