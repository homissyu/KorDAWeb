        function sendAjax(){
            // content-type을 설정하고 데이터 송신
            var xhr = new XMLHttpRequest();
            xhr.open('POST', document.URL.split("#")[0]+'getData');
            xhr.setRequestHeader('Content-type', "application/json");
            xhr.send();
            
            var tempObjArr3 = document.getElementsByClassName("rightmenu")[0].getElementsByTagName("ul")[0].getElementsByTagName("li");
            // 데이터 수신이 완료되면 표시
            xhr.addEventListener('load', function(){
                ajaxRet = JSON.parse(xhr.responseText);
                // "pegDon","pesDon","goldBuy","goldSell","excRate","investRate","kospi","kosdaq","dji","nasdaq","btc","wti","pegGram","pesGram","s_p500","eth"var tempClass;
                var tempColor;
                var tempFontWeight;
                var tempVal = 0;
                var tempStr;
                var tempObj;
                
                for(var i=0 ; i<tempObjArr3.length;i++){
                    // if(originArr.length>0)alert(tempStr +":"+originArr[i]+":"+ajaxRet[i].thisValue);
                    tempObj = tempObjArr3[i].getElementsByTagName("span")[1];
                    tempStr = Number.parseFloat(tempObj.innerText);
                    tempVal = originArr3[i] - ajaxRet[i].thisValue;
                    
                    originArr3.reduce[i];
    
                    if(tempStr!=0 && tempVal>0){
                        tempColor = "#87dfff";
                        tempClass = "blinkText";
                        tempFontWeight = "bold";
                    }else if(tempStr!=0 && tempVal<0){
                        tempColor = "#ff7575";
                        tempClass = "blinkText";
                        tempFontWeight = "bold";
                    }else{
                        tempColor = "white";
                        tempClass = "";
                        tempFontWeight = "";
                    }
    
                    originArr3[i] = ajaxRet[i].thisValue;
                    tempObj.innerText = new Intl.NumberFormat('ko-KR', { style: 'decimal', maximumFractionDigits: 2}).format(ajaxRet[i].thisValue)
                    tempObj.style.color =  tempColor;
                    tempObj.className = tempClass;
                    tempObj.style.fontWeight = tempFontWeight;
                    // alert(tempObjArr.length+":"+ajaxRet[i].thisValue+":"+tempObj.innerText);
                }
			});
		}
		
		function sendAjax4Asset(){
            // content-type을 설정하고 데이터 송신
            var xhr = new XMLHttpRequest();
            xhr.open('POST', document.URL.split("#")[0]+'getAsset');
            xhr.setRequestHeader('Content-type', "application/json");
            xhr.send();
			
			//egold
            var egoldCardinal = document.getElementsByClassName("counter counter-lined")[0].getElementsByTagName("span")[0];
			var egoldDecimal = document.getElementsByClassName("counter counter-lined")[0].getElementsByTagName("span")[1];
            //esilver
            var esilverCardinal = document.getElementsByClassName("counter counter-lined")[1].getElementsByTagName("span")[0];
			var esilverDecimal = document.getElementsByClassName("counter counter-lined")[1].getElementsByTagName("span")[1];
			//point
            var point = document.getElementsByClassName("counter counter-lined")[2].getElementsByTagName("span")[0];
			
			// 데이터 수신이 완료되면 표시
