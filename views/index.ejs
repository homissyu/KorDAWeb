            hour = hour >= 10 ? hour : '0' + hour;          //hour 두자리로 저장
            var min = date.getMinutes();
            min = min >= 10 ? min : '0' + min;          //min 두자리로 저장
            var sec = date.getSeconds();
            sec = sec >= 10 ? sec : '0' + sec;          //sec 두자리로 저장
            return  year + '.' + month + '.' + day;
        }
        function getNow(){
            return getFormatDate(new Date());
        }
    </script>
