<script type="text/javascript">
    var ajaxRet;
    var originArr = new Array();
    var newArr = new Array();

    function sendAjax(){
        // content-type을 설정하고 데이터 송신
        var xhr = new XMLHttpRequest();
        xhr.open('POST', document.URL.split("#")[0]+'getData');
        xhr.setRequestHeader('Content-type', "application/json");
        xhr.send();
        
        var tempObjArr = document.getElementsByClassName("left-menu-bar__contents-list")[0].getElementsByTagName("ul")[0].getElementsByTagName("li");
        var tempObjArr2 = document.getElementsByClassName("pr");
        // 데이터 수신이 완료되면 표시
        xhr.addEventListener('load', function(){
            ajaxRet = JSON.parse(xhr.responseText);
            // "pegDon","pesDon","goldBuy","goldSell","excRate","investRate","kospi","kosdaq","dji","nasdaq","btc","wti","pegGram","pesGram","s_p500","eth"var tempClass;
            var tempColor;
            var tempFontWeight;
            var tempVal = 0;
            var tempStr;
            var tempObj;
            
            for(var i=0 ; i<tempObjArr.length;i++){
                // if(originArr.length>0)alert(tempStr +":"+originArr[i]+":"+ajaxRet[i].thisValue);
                tempObj = tempObjArr[i].getElementsByTagName("span")[1];
                tempStr = Number.parseFloat(tempObj.innerText);
                tempVal = originArr[i] - ajaxRet[i].thisValue;
                
                originArr.reduce[i];

                if(tempStr!=0 && tempVal>0){
                    tempColor = "blue";
                    tempClass = "blinkText";
                    tempFontWeight = "bold";
                }else if(tempStr!=0 && tempVal<0){
                    tempColor = "red";
                    tempClass = "blinkText";
                    tempFontWeight = "bold";
                }else{
                    tempColor = "black";
                    tempClass = "";
                    tempFontWeight = "";
                }

                originArr[i] = ajaxRet[i].thisValue;
                tempObj.innerText = new Intl.NumberFormat('ko-KR', { style: 'decimal', maximumFractionDigits: 2}).format(ajaxRet[i].thisValue)
                tempObj.style.color =  tempColor;
                tempObj.className = tempClass;
                tempObj.style.fontWeight = tempFontWeight;
                // alert(tempObjArr.length+":"+ajaxRet[i].thisValue+":"+tempObj.innerText);
            }

            for(var k=0;k<tempObjArr2.length;k+=2){
                tempObjArr2[k].innerHTML = new Intl.NumberFormat('ko-KR', { style: 'decimal', maximumFractionDigits: 2}).format(ajaxRet[0].thisValue);
                tempObjArr2[k+1].innerHTML = new Intl.NumberFormat('ko-KR', { style: 'decimal', maximumFractionDigits: 2}).format(ajaxRet[1].thisValue);
            }
        });
    }
    window.setInterval("sendAjax()", 10000);
</script>

